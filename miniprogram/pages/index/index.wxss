/**index.wxss**/

page {
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height: 100%;
  width: 100%;
}

#map-container{
  height: 100%;
  width: 100%;
}

.map{
  width: 100%;
  height: 100vh;
}
#setPosition{
  background: white;
  height: 40px;
  width: 40px;
  border-radius:50%;
  display: flex;
  align-items: center;
  justify-content: center;
  position: absolute;
  right: 20px;
  bottom: 140px;
}

#mainbar{
  height: 100px;
  width: 100%;
  position: absolute;
  bottom: 0px;
  background-color: white;
  display: flex;
  justify-content: space-around;
  align-items: flex-start;
  border-radius: 20px 20px 0 0;
  margin-bottom: -13px;
  padding-top: 20px;
}
#analyzeBtn{
  height: 60px;
  width: 160px;
  background-color: #ffc8c4;
  color: white;
  font-size: 20px;
  font-weight: 600;
  font-family: Arial, Helvetica, sans-serif;
  border-radius: 60px 60px;
  text-align: center;
  line-height: 60px;
  letter-spacing: 4px;
}