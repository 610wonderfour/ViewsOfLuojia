/* pages/signIn/signIn.wxss */
#container{
  height: 100vh;
  width: 100%;
  padding: 0;
  margin: 0;
  position: relative;
}
#welcomeTxt{
  height: 50px;
  width: 300px;
  font-size: 25px;
  position: absolute;
  top: 30%;
  left: 50%;
  transform: translate(-50%,-50%);
  text-align: center;
}
#signInBtn{
  height: 50px;
  width: 330px;
  position: absolute;
  top: 41%;
  left: 50%;
  transform: translate(-50%,-50%);
  font-size: 18px;
  font-weight: 350;
  letter-spacing: 1px;
  display: flex;
  justify-content: center;
  align-items: center;
}
#wechatIcon{
  height: 20px;
  width: 20px;
  padding: 10px;
}